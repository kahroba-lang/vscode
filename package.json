{
  "name": "kahroba",
  "displayName": "Kahroba",
  "description": "Kahroba language support for Visual Studio Code",
  "version": "1.0.1",
  "publisher": "AmirhosseinMalekian",
  "repository": {
    "type": "git",
    "url": "https://github.com/kahroba-lang/vscode"
  },
  "icon": "images/kahroba.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "kahroba",
        "aliases": [
          "Kahroba",
          "kahroba",
          "KAHROBA"
        ],
        "extensions": [
          ".kahroba",
          ".khb"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kahroba",
        "scopeName": "source.kahroba",
        "path": "./syntaxes/kahroba.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "kahroba",
        "path": "./snippets/kahroba.json"
      }
    ]
  }
}
